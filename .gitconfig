[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
[commit]
	gpgsign = true
[user]
	email = binota@binota.org
	name = BinotaLIU
[core]
	autocrlf = input
	fileMode = false
	editor = vim
[init]
	defaultBranch = main
[gpg]
	program = /usr/local/MacGPG2/bin/gpg
